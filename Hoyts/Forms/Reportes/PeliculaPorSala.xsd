                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        F.id, P.titulo_original, F.fecha_funcion, D.nombre AS Dia, F.horario_comienzo, F.horario_fin, Fv.nombre AS FormatoVideo, Fa.nombre AS FormatoAudio, F.stock_tickets
FROM            Funciones AS F INNER JOIN
                         Pelicula AS P ON P.id = F.id_pelicula INNER JOIN
                         FormatoVideo AS Fv ON Fv.id = F.id_formato INNER JOIN
                         FormatoAudio AS Fa ON Fa.id = F.id_audio INNER JOIN
                         dias AS D ON D.number = F.dia
WHERE        (F.id_sala = @id_sala) AND (F.fecha_funcion &gt;= CONVERT(datetime, @fecha_inicio, 105)) AND (F.fecha_funcion &lt;= CONVERT(datetime, @fecha_fin, 105))
